{% extends '::base.html.twig' %}

{% block title %}
    {{ parent() }} - {{ team.name }}  national football team
{% endblock %}
{% block header %}{{ team.name }} national football team{% endblock %}
{% block body %}
    <p>
        {{ team.info }}
    </p>
    <p>
        <ul>
            <li><a href="{{ playersRoute }}{{ team.name }}">Team players</a></li>
            <li><a href="{{ coachsRoute }}{{ team.name }}">Team coachs</a></li>
        </ul>
    </p>
{% endblock %}